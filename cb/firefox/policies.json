{
  "policies": {
    "BlockAboutAddons": true,
    "BlockAboutConfig": true,
    "BlockAboutProfiles": true,
    "Cookies": {
      "AcceptThirdParty": "from-visited",
      "ExpireAtSessionEnd": true,
      "RejectTracker": true,
      "Locked": true
    },
    "DisableSetDesktopBackground": true,
    "DisableMasterPasswordCreation": true,
    "DisableDeveloperTools": true,
    "DisableFirefoxAccounts": true,
    "DisablePocket": true,
    "DisablePrivateBrowsing": true,
    "DisableSafeMode": true,
    "DNSOverHTTPS": {
      "Enabled": true,
      "ProviderURL": "https://family.cloudflare-dns.com/dns-query",
      "Locked": true
    },
    "EnableTrackingProtection": {
      "Value": true,
      "Locked": true,
      "Cryptomining": true,
      "Fingerprinting": true
    },
    "ExtensionSettings": {
      "*": {
        "blocked_install_message": "L'installation d'extensions n'est pas autorisée.",
        "installation_mode": "blocked"
      },
      "uBlock0@raymondhill.net": {
        "installation_mode": "force_installed",
        "install_url": "https://addons.mozilla.org/firefox/downloads/latest/ublock-origin/latest.xpi"
      },
      "langpack-fr@firefox.mozilla.org": {
        "installation_mode": "force_installed",
        "install_url": "https://addons.mozilla.org/firefox/downloads/latest/français-language-pack/latest.xpi"
      },
      "French-GC@grammalecte.net": {
        "installation_mode": "force_installed",
        "install_url": "https://addons.mozilla.org/firefox/downloads/latest/grammalecte-fr/latest.xpi"
      }
    },
    "ExtensionUpdate": true,
    "FirefoxHome": {
      "Search": true,
      "TopSites": false,
      "Highlights": false,
      "Pocket": false,
      "Snippets": false,
      "Locked": true
    },
    "Homepage": {
      "Locked": true,
      "StartPage": "homepage-locked"
    },
    "PrimaryPassword": false,
    "NetworkPrediction": false,
    "NewTabPage": true,
    "NoDefaultBookmarks": true,
    "OfferToSaveLogins": false,
    "OverridePostUpdatePage": "",
    "PasswordManagerEnabled": false,
    "PSFjs": {
      "Enabled": false
    },
    "PopupBlocking": {
      "Allow": ["https://mail.google.com"],
      "Default": true,
      "Locked": false
    },
    "PromptForDownloadLocation": false,
    "Proxy": {
      "Mode": "system",
      "Locked": true,
      "Passthrough": "",
      "AutoLogin": false,
      "UseProxyForDNS": false
    },
    "RequestedLocales": "fr-FR,fr,en-US,en",
    "SanitizeOnShutdown": true,
    "SearchBar": "unified",
    "SearchSuggestEnabled": true,
    "ShowHomeButton": false,
    "UserMessaging": {
      "WhatsNew": false,
      "ExtensionRecommendations": false,
      "FeatureRecommendations": false,
      "UrlbarInterventions": false,
      "SkipOnboarding": false
    }
  }
}
